<configuration>

    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date %level [%thread] %logger{20} - %msg%n</pattern>
        </encoder>
    </appender>

	<logger name="org.subtlelib.poiooxml" level="debug" />

    <root level="info">
        <appender-ref ref="console" />
    </root>
    
</configuration>
